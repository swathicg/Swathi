/*Generating unicode.java*/
public class Unicode
{
    public static void main(String args[])
    {
      System.out.println("Letters\tUnicode");
      for(int i=0;i < 26;i++)
      {
          System.out.println((char)('A'+i) +"\t"+('A'+i));
      }
      System.out.println("****************************");
      System.out.println("Letters\tUnicode");
      for(int i=0;i < 26;i++)
      {
          System.out.println((char)('a'+i) +"\t"+('a'+i));
      }
    }
}
